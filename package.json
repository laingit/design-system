  "scripts": {
    "build": "npm run gulp -- build",
    "dist": "npm run gulp -- dist",
    "gulp": "babel-node ./node_modules/gulp/bin/gulp.js",
    "lint": "npm run gulp -- lint",
    "start": "npm run build && npm run previewer",
    "previewer": "babel-node scripts/previewer",
    "previewer-dev": "SLDS_PREVIEWER=development babel-node scripts/previewer",
    "stats": "npm run gulp -- stylestats",
    "pretest": "npm run gulp -- build",
    "test": "node -r @std/esm ./node_modules/.bin/jest",
    "jest": "node -r @std/esm ./node_modules/.bin/jest",
    "precommit": "npm run release-notes:hook && lint-staged",
    "pr-checks": "npm run gulp -- pr-checks",
    "compile-aura-tokens-map": "npm run gulp -- generate:auraTokensMap",
    "compile-token-component-map": "npm run gulp -- generate:tokenComponentMap",
    "releasenotes": "node ./scripts/release-notes/index.js",
    "storybook": "npm run build && start-storybook -s ./.dist -p 9001 -c .storybook",
    "storybook-build": "build-storybook -c .storybook -o .out",
    "release-notes:compile": "node ./scripts/release-notes/index.js",
    "release-notes:hook": "node ./scripts/release-notes/hook.js"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,json}": [
        "prettier --single-quote --write",
        "git add"
      ]
    }
  },
  "jest": {
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*(test|spec))\\.jsx?$",
    "globalSetup": "./jest.setup.global.js",
    "globalTeardown": "./jest.teardown.global.js",
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/shared/vendor/prism/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/shared/vendor/prism/"
    ]
  },
  "@std/esm": {
    "cjs": true,
    "esm": "js"
  }
